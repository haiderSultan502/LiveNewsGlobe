<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/complete_item_click_city"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_50sdp"
    android:layout_marginLeft="@dimen/_17sdp"
    android:layout_marginRight="@dimen/_14sdp">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_45sdp"
        android:background="@drawable/list_bgg">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_city"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_centerInParent="true">

        </androidx.appcompat.widget.AppCompatImageView>
        <TextView
            android:id="@+id/text_view_city_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Los Angeles"
            android:textColor="#18204E"
            android:layout_centerInParent="true"
            android:textSize="@dimen/_15sdp" />
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_city_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/cities_icon"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_24sdp"
            android:layout_centerInParent="true">

        </androidx.appcompat.widget.AppCompatImageView>




    </RelativeLayout>
</LinearLayout>

